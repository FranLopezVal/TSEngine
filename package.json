    "iObject": "./src/core/ifaces/iobject.js",
    "iComponent": "./src/core/ifaces/icomponent.js"
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
