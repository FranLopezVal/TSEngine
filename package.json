    "@types/node": "^20.10.4",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "enginets"
  ],
  "name": "enginets",
  "version": "0.2.0",
  "module": "commonjs",
  "declaration": true,
  "strict": true,
  "main": "build/main.js",
